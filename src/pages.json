{
	"pages": [
		{
			"path": "pages/index/index",
			"type": "home",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/code/index",
			"type": "page",
			"style": {
				"navigationBarTitleText": "示例"
			},
			"layout": "default"
		},
		{
			"path": "pages/example/alova",
			"type": "page",
			"style": {
				"navigationBarTitleText": "Alova"
			},
			"layout": "example"
		},
		{
			"path": "pages/example/hook",
			"type": "page",
			"style": {
				"navigationBarTitleText": "Hook"
			},
			"layout": "example"
		},
		{
			"path": "pages/example/layout",
			"type": "page",
			"style": {
				"navigationBarTitleText": "Layout"
			},
			"layout": false
		},
		{
			"path": "pages/example/pinia",
			"type": "page",
			"style": {
				"navigationBarTitleText": "Pinia"
			},
			"layout": "example"
		},
		{
			"path": "pages/example/router",
			"type": "page",
			"style": {
				"navigationBarTitleText": "Router"
			},
			"layout": "example"
		},
		{
			"path": "pages/example/z-paging",
			"type": "page",
			"style": {
				"navigationBarTitleText": "Z-PAGING"
			},
			"layout": "example"
		},
		{
			"path": "pages/common/webview/index",
			"type": "page"
		}
	],
	"globalStyle": {
		"navigationBarBackgroundColor": "#f8f8f8",
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-fastkit",
		"backgroundColor": "#f8f8f8",
		"backgroundTextStyle": "dark",
		"backgroundColorTop": "#f8f8f8",
		"backgroundColorBottom": "#f8f8f8",
		"enablePullDownRefresh": false,
		"onReachBottomDistance": 50,
		"animationType": "pop-in",
		"animationDuration": 300
	},
	"tabBar": {
		"backgroundColor": "#f8f8f8",
		"borderStyle": "black",
		"color": "#72757e",
		"selectedColor": "#6b69ef",
		"list": [
			{
				"iconPath": "static/tabbar/home-outline-rounded.png",
				"selectedIconPath": "static/tabbar/home-rounded.png",
				"pagePath": "pages/index/index"
			},
			{
				"iconPath": "static/tabbar/code-blocks-outline.png",
				"selectedIconPath": "static/tabbar/code-blocks.png",
				"pagePath": "pages/code/index"
			}
		]
	},
	"easycom": {
		"custom": {
			"^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
		}
	},
	"subPackages": [
		{
			"root": "pages-sub",
			"pages": [
				{
					"path": "demo/icon",
					"type": "page",
					"style": {
						"navigationBarTitleText": "iconify"
					},
					"layout": "example"
				},
				{
					"path": "demo/unocss",
					"type": "page",
					"style": {
						"navigationBarTitleText": "UnoCSS"
					},
					"layout": "example"
				}
			]
		}
	]
}