    "build:quickapp-webview-union": "uni build -p quickapp-webview-union",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "uvm": "node ./scripts/update.mjs",
    "cz": "git pull && czg"
